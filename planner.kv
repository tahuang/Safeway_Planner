#:kivy 1.10.0

<GroceryList>:    
    canvas:
    	Color:
    		rgba: 1,1,1,1
    	Rectangle:
    		pos: self.pos
    		size: self.size

    orientation: 'vertical'
    BoxLayout:
        size_hint: 1, .1
